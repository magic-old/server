server {
  listen 80;
  listen [::]:80;
  server_name HOSTNAME;
  root /www/data;
  index index.html;

  location / {
    try_files /ROOT_DIR$uri /ROOT_DIR$uri.html /ROOT_DIR$uri/index.html =404;
  }
  location = / {
    try_files /ROOT_DIR/index.html =404;
  }
  error_page 404 /ROOT_DIR/index.html;
}
